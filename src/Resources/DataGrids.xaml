			<DataTrigger Binding="{Binding Status}" Value="old">
				<Setter Property="Background" Value="{StaticResource RedColorBrush}"/>
			</DataTrigger>
		</Style.Triggers>
		<Setter Property="ContextMenu" Value="{StaticResource DemoRowContextMenu}" />
	</Style>

